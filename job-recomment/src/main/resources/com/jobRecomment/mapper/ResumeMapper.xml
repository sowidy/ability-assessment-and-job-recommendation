<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobRecomment.mapper.ResumeMapper">

    <insert id="insertAndGetId" useGeneratedKeys="true" keyProperty="id" >
        INSERT INTO resume (origin_name, link_name, create_time)
        VALUES (#{originName}, #{linkName}, #{createTime})
    </insert>
</mapper>
